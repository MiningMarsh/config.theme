Xft*dpi:                129
Xft*antialias:          true
Xft*hinting:            true
*font:                  xft:DejaVu Sans Mono:size=9

! URxvt*depth:            32
URxvt*background:       rgba:3800/3a00/3b00/ffff
URxvt.letterSpace:      2

! Linux color scheme.
*foreground:            #b2b2b2
*background:            #383a3b

! Black + DarkGrey
*color0:                #383a3b
*color8:                #686868

! DarkRed + Red
*color1:                #b21818
*color9:                #ff5454

! DarkGreen + Green
*color2:                #18b218
*color10:               #54ff54

! DarkYellow + Yellow
*color3:                #b26818
*color11:               #ffff54

! DarkBlue + Blue
*color4:                #1818b2
*color12:               #5454ff

! DarkMagenta + Magenta
*color5:                #b218b2
*color13:               #ff54ff

! DarkCyan + Cyan
*color6:                #18b2b2
*color14:               #54ffff

URxvt*scrollBar:        false

Xcursor.theme:          Obsidian
